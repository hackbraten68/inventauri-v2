{
  "name": "inventauri-v2",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "dotenv -e .env.local -- astro dev",
    "build": "dotenv -e .env.local -- astro build",
    "preview": "dotenv -e .env.local -- astro preview",
    "astro": "astro",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "test": "dotenv -e .env.local -- vitest run",
    "test:watch": "dotenv -e .env.local -- vitest",
    "test:contracts": "dotenv -e .env.local -- vitest run tests/contracts"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.4",
    "@astrojs/react": "^4.3.1",
    "@astrojs/tailwind": "^6.0.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.57.4",
    "astro": "^5.13.10",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.1",
    "prisma": "^6.16.2",
    "tsx": "^4.20.5",
    "vitest": "^2.0.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
